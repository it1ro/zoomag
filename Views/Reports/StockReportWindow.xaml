<Window x:Class="Zoomag.Views.Reports.StockReportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Отчет по складу" Height="450" Width="800">
    <DockPanel>
        <StackPanel DockPanel.Dock="Right" Width="200" Margin="10">
            <Button x:Name="ExportToExcelButton" Content="Вывод в Excel" Click="ExportToExcel" />
            <Button x:Name="BackButton" Content="Назад" Click="GoToAdmin" />
        </StackPanel>
        <DataGrid x:Name="StockReportGrid" AutoGenerateColumns="False"/>
    </DockPanel>
</Window>